  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>IT18128796_dw</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CUSTOMER_ID">
            <Value>=Parameters!CUSTOMER_ID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT DCD.AlternativeCustomerID, FOD.QTY, FOD.AMOUNT
FROM     FactOrderDetails AS FOD INNER JOIN
                  DimCustomerDetails AS DCD ON FOD.CUSTOMER_ID = DCD.AlternativeCustomerID INNER JOIN
                  DimProductDetail AS DPD ON FOD.PRODUCT_ID = DPD.AlternativeProductId INNER JOIN
                  DimDate AS DD ON FOD.ORDER_DATE = DD.DateKey
WHERE  (FOD.CUSTOMER_ID IN (@CUSTOMER_ID))</CommandText>
      </Query>
      <Fields>
        <Field Name="AlternativeCustomerID">
          <DataField>AlternativeCustomerID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="QTY">
